{
    "name": "existenznl/permcheck",
    "description": "PermCheck is a tool to validate the executable bit of files in a PHP project",
    "license": "MIT",
    "require": {
        "symfony/console": "^2.1|^3.0|^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^2.1|^3.0|^4.0|^5.0|^6.0"
    },
    "authors": [
        {
            "name": "Stefan van Essen",
            "email": "git@stefan-van-essen.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "eXistenZNL\\PermCheck\\": "src/"
        }
    },
    "bin": [
        "bin/permcheck"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7.1",
        "mockery/mockery": "^1.5.1"
    }
}
